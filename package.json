{
  "name": "webpack-4",
  "version": "1.0.0",
  "description": "Curso de webpack-4",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "build:manual": "webpack --entry ./index.js --output ./bundle.js --mode development",
    "build:external:dev": "webpack --config ./external/dev.config.js",
    "build:external:prod": "webpack --config ./external/prod.config.js",
    "build:multi-entry-points": "webpack --config ./multi-entry-points/webpack.config.js --mode production",
    "build:css-style-loader": "webpack --config ./css-style-loader/webpack.config.js --mode production",
    "build:plugins": "webpack --config ./plugins/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:hot": "webpack --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack-dev-server --config ./babel/webpack.config.js",
    "build:react": "webpack-dev-server --config ./react/webpack.config.js",
    "build:react-app": "webpack-dev-server --config ./react-app/webpack.config.js"

  },
  "keywords": [
    "webpack",
    "curso",
    "webpack-4",
    "platzi"
  ],
  "author": "Fadith Escorcia",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-react": "7.8.3",
    "babel-loader": "8.0.6",
    "css-loader": "3.4.2",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.9.0",
    "style-loader": "1.1.3",
    "webpack": "4.32.2",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.10.3"
  },
  "dependencies": {
    "@babel/runtime": "7.8.7",
    "react": "16.13.0",
    "react-dom": "16.13.0"
  }
}
